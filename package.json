{
  "name": "@mharj/openweathermap",
  "version": "0.1.3",
  "description": "Open Weather API Client",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    "types": {
      "import": "./dist/index.d.mts",
      "require": "./dist/index.d.cts"
    },
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "scripts": {
    "docs": "typedoc",
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest test --run --no-isolate --coverage",
    "coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
    "lint": "biome check",
    "validate": "tsgo --noEmit --project tsconfig.test.json"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/openweathermap",
  "keywords": [
    "weather",
    "api",
    "client"
  ],
  "author": "mharj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mharj/ts-openweather/issues"
  },
  "homepage": "https://github.com/mharj/ts-openweather#readme",
  "devDependencies": {
    "@avanio/expire-cache": "^0.6.9",
    "@avanio/logger-like": "^0.2.12",
    "@biomejs/biome": "2.3.15",
    "@luolapeikko/cache-types": "^0.0.9",
    "@luolapeikko/result-option": "^2.2.1",
    "@luolapeikko/ts-common": "^1.1.4",
    "@types/node": "^24",
    "@typescript/native-preview": "7.0.0-dev.20260214.1",
    "@vitest/coverage-v8": "^4.0.18",
    "c8": "^10.1.3",
    "source-map-support": "^0.5.21",
    "tsdown": "^0.20.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.17",
    "typedoc-plugin-zod": "^1.4.3",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@luolapeikko/cache-types": "^0.0",
    "@luolapeikko/result-option": ">= 1",
    "@luolapeikko/ts-common": ">= 0.0.1",
    "zod": "^4"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
