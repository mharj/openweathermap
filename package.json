{
  "name": "@mharj/openweathermap",
  "version": "0.1.2",
  "description": "Open Weather API Client",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    "types": {
      "import": "./dist/index.d.mts",
      "require": "./dist/index.d.cts"
    },
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "scripts": {
    "doc": "typedoc",
    "build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest test --run --no-isolate --coverage",
    "coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/openweathermap",
  "keywords": [
    "weather",
    "api",
    "client"
  ],
  "author": "mharj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mharj/ts-openweather/issues"
  },
  "homepage": "https://github.com/mharj/ts-openweather#readme",
  "devDependencies": {
    "@avanio/expire-cache": "^0.6.9",
    "@avanio/logger-like": "^0.2.12",
    "@cspell/eslint-plugin": "^8.19.4",
    "@eslint/js": "^9.39.2",
    "@luolapeikko/cache-types": "^0.0.8",
    "@luolapeikko/result-option": "^2.2.1",
    "@luolapeikko/ts-common": "^0.3.9",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/node": "^22.19.11",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sonarjs": "^3.0.7",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "tsdown": "^0.20.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-zod": "^1.4.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.4.1",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@luolapeikko/cache-types": "^0.0",
    "@luolapeikko/result-option": ">= 2",
    "@luolapeikko/ts-common": ">= 0.0.1",
    "zod": "^3"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
